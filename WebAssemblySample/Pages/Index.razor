@page "/"

    <BlazorTabulator.Tabulator tableData="@sampleData">
        <BlazorTabulator.Data.TabulatorColumnData field="name" title="Name"></BlazorTabulator.Data.TabulatorColumnData>
    </BlazorTabulator.Tabulator>

@code {

    private TabulatorData sampleData = new TabulatorData()
    {
        // This could be an API call instead of hardcoded values.
        Dataset = new List<Person>() {
            new Person() { Name = "Nick" },
            new Person() {  Name = "Joe" }
        }
    };
}

